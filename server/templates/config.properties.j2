{% if presto__coordinator_group in group_names %}
coordinator=true
discovery-server.enabled=true
{% else %}
coordinator=false
{% endif %}
datasources=jmx,hive
http-server.http.port={{ presto__http_port }}
presto-metastore.db.type=h2
presto-metastore.db.filename=var/db/MetaStore
task.max-memory={{ presto__task_max_memory }}
discovery.uri=http://{{ coordinator_fqdn }}:{{ presto__http_port }}
